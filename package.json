{
   "name": "ngtools-webpack-winbug",
   "version": "0.0.1",
   "description": "Sample angular webpack app to show windows hangs on 95% emitting when devtools ist set to source-map",
   "main": "index.js",
   "scripts": {
      "clean": "rimraf dist/*.js",
      "start": "npm run clean && webpack --config webpack.config.js --progress",
      "serve": "lite-server -c dist/bs-config.json"
   },
   "keywords": ["angular", "webpack2", "windows", "bug"],
   "author": "mariussl",
   "license": "",
   "dependencies": {
      "@angular/common": "2.3.1",
      "@angular/compiler": "2.3.1",
      "@angular/compiler-cli": "2.3.1",
      "@angular/core": "2.3.1",
      "@angular/platform-browser": "2.3.1",
      "@angular/platform-browser-dynamic": "2.3.1",
      "@angular/tsc-wrapped": "^0.5.0",
      "core-js": "2.4.1",
      "rxjs": "5.0.0-rc.4",
      "zone.js": "0.7.2",
      "@ngtools/webpack": "1.2.1",
      "@types/node": "6.0.45",
      "raw-loader": "0.5.1",
      "typescript": "2.0.3",
      "webpack": "2.1.0-beta.25",
      "rimraf": "2.5.2",
      "lite-server": "2.2.2"
   }
}
